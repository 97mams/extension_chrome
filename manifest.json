{
  "manifest_version": 3,
  "name": "extensionfor my project Keno",
  "version": "1.0",
  "description": "juste fun project for Keno",
  "pemission": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "img/keno-icon.png"
    },
    "default_title": "keno"
  },
  "icons": {
    "48": "img/keno.png",
    "128": "img/keno-icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}